{
    "env" : {
        "browser": true,
        "node": true
    },
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
    "extends": [
        "standard",
        "prettier",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0
    }
}